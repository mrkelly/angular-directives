{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "no-use-before-define": 0,
    "no-console": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}]
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "angular": true
  }
}
